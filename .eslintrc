{
  "extends": "airbnb-base",
  "env": {
    'browser': true
  },
  "globals": {
    "createjs": false
  },
  "rules": {
    "id-length": 0,
    "func-names": 0,
    "default-case": 0,
    "no-param-reassign": 0,
    "no-mixed-operators": 0,
    "max-len": 1,
    "guard-for-in": 1,
    "no-use-before-define": [2, "nofunc"]
  }
}
